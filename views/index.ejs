<h1 id="index-main-heading">Welcome to CSE Motors!</h1>
<%- messages() %>
    <section class="hero">
        <div class="call-to-action-container">
            <h2>DMC Delorean</h2>
            <p>3 Cup Holders<br>Superman doors<br>Fuzzy dice!</p>
            <button type="button" class="button-styles">Own Today</button>
        </div>
    </section>

    <section class="reviews-upgrades">
        <div class="upgrades">
            <h3>Delorean Upgrades</h3>
            <div class="upgrade-grid">
                <div class="upgrade-item">
                    <a href="#"><img src="images/upgrades/flux-cap.png" alt="Flux Capacitor"></a>
                    <a href="#">Flux Capacitor</a>
                </div>
                <div class="upgrade-item">
                    <a href="#"><img src="images/upgrades/flame.jpg" alt="Flame Decals"></a>
                    <a href="#">Flame Decals</a>
                </div>
                <div class="upgrade-item">
                    <a href="#"><img src="images/upgrades/bumper_sticker.jpg" alt="Bumper Stickers"></a>
                    <a href="#">Bumper Stickers</a>
                </div>
                <div class="upgrade-item">
                    <a href="#"><img src="images/upgrades/hub-cap.jpg" alt="Hub Caps"></a>
                    <a href="#">Hub Caps</a>
                </div>
            </div>
        </div>
        <div class="reviews">
            <h3>DMC Delorean Reviews</h3>
            <div class="review-wrapper">
                <ul>
                    <li>"So fast it's almost like traveling in time." (4/5)</li>
                    <li>"Coolest ride on the road." (4/5)</li>
                    <li>"I'm feeling McFly!" (5/5)</li>
                    <li>"The most futuristic ride of our day." (4.5/5)</li>
                    <li>"80's livin and I love it!" (5/5)</li>
                </ul>
            </div>
        </div>

    </section>